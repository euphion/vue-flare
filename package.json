{
  "name": "vue-flare",
  "private": false,
  "version": "1.0.1",
  "license": "MIT",
  "author": "Jakub Dvorak (https://www.linkedin.com/in/jakubdvorakone)",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-flare.umd.js",
  "module": "./dist/vue-flare.es.js",
  "exports": {
    ".": {
      "require": "./dist/vue-flare.umd.js",
      "import": "./dist/vue-flare.es.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/euphion/vue-flare.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:demo": "vue-tsc && vite build --config ./vite.demo-config.js",
    "preview": "vite preview",
    "preview:demo": "vite preview --config ./vite.demo-config.js"
  },
  "dependencies": {
    "eslint-plugin-vue": "^9.9.0",
    "pinia": "^2.0.33",
    "sass": "^1.58.3",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.36.0",
    "firebase-tools": "^11.25.1",
    "install": "^0.13.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^1.0.24"
  }
}
